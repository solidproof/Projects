interface IDexFactory {
    function createPair(address tokenA, address tokenB)
        external
        returns (address pair);
    function pairFor(address tokenA, address tokenB) external view returns (address pair);
    function getPair(address tokenA, address tokenB) external view returns (address pair);
}

interface IDexRouter {
     function WETH() external pure returns(address);
     function factory() external pure returns (address);
     function swapExactTokensForETH(uint amountIn, uint amountOutMin, address[] calldata path, address to, uint deadline) external returns (uint[] memory amounts);
     function addLiquidityETH(
        address token,
        uint256 amountTokenDesired,
        uint256 amountTokenMin,
        uint256 amountETHMin,
        address to,
        uint256 deadline
    )
        external
        payable
        returns (
            uint256 amountToken,
            uint256 amountETH,
            uint256 liquidity
        );

    function swapExactTokensForTokensSupportingFeeOnTransferTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external;

    function swapExactETHForTokensSupportingFeeOnTransferTokens(
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external payable;

    function swapExactTokensForETHSupportingFeeOnTransferTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external;
} 

interface IDexPair{
    function token0() external view returns (address);
    function token1() external view returns (address);
    function getReserves() external view returns (uint112 reserve0, uint112 reserve1, uint32 blockTimestampLast);
}




contract Mickey is ERC20, ReentrancyGuard, Ownable {

    using SafeMath for uint256;

    address public swapRouter;
    address public weth;
    address public tokenPair;
    address public fundAddr;
    uint256 public fundRatio;

    
    uint256 public ethMintLimitPerAddress;
    uint256 public price;
    uint256 public amountPerUnits;

    uint256 public mintSupply;
    uint256 public minted;

    bool public lpDeployed;
    address public launcher;

    event MintEvent(address minter,uint256 amount, uint256 cost);
    event RefundEvent(address refundee, uint256 value, uint256 _eth);
    event LunchEvent(address indexed to, uint256 amount, uint256 ethAmount, uint256 liquidity);

    constructor()ERC20("Mickey on Base ", "MICKEY"){
        price = 1000000000000000;
        amountPerUnits = 50000*10**18;
        lpDeployed = false;
        fundRatio = 2000;
        fundAddr = 0xEfA556a802ef9a36d68127055869e8CF028B9CD7;
        mintSupply = 5000000000*10**18;        
        uint256 totalSupply = mintSupply * 2;
        _mint(address(this), totalSupply);
        launcher = msg.sender;
        ethMintLimitPerAddress=0.5 ether;


        swapRouter = 0x4752ba5DBc23f44D87826276BF6Fd6b1C372aD24; 
        weth = IDexRouter(swapRouter).WETH();
        tokenPair = IDexFactory(IDexRouter(swapRouter).factory()).createPair(weth, address(this));
        _approve(address(this), swapRouter, totalSupply);
    }
        

    receive() external payable {
        if(!lpDeployed){
            if (msg.value == 0.000123 ether) {
                    require(msg.sender == launcher, "only launcher can start before mint is completed");
                    start();
            } else {
                mint();
                if(minted == mintSupply){ // last mint tx deploys the LP to uniswap
                    start();
                }
            }
        }
    }

    function unmintedSupply() public view returns(uint256) {
        return mintSupply - minted;
    } 


    function mint() internal nonReentrant {
        require(msg.value >= price, "value less than minimum price 0.001 Eth");
        // mint must not be over
        require(!lpDeployed, "mint is over");

        uint256 units = msg.value / price;
        uint256 cost = units * price;
        uint256 refund = msg.value - cost;

        uint256 fund = cost.mul(fundRatio).div(10000);
        payable(fundAddr).transfer(fund);

        require(
            minted + units * amountPerUnits <= mintSupply,
            "exceeds max mint supply"
        );

        require(
            balanceOf(msg.sender) * price / amountPerUnits  + cost <= ethMintLimitPerAddress,
            "Max mint per address is 0.5 Eth"
        );

        _transfer(address(this), msg.sender, units * amountPerUnits);
        minted += units * amountPerUnits;

        emit MintEvent(msg.sender, units * amountPerUnits, cost);

        if (refund > 0) {
            payable(msg.sender).transfer(refund);
        }
    }

     function start() internal {
        require(!lpDeployed, "Lp has has already been deployed");
        
        // set lpDeployed
        lpDeployed = true;
        uint256 lp = minted.mul(10000 - fundRatio).div(10000);

        uint256 balance = balanceOf(address(this));
        uint256 diff = balance - lp;
        // burn diff
        _burn(address(this), diff);
        
        // add liquidity
        (uint256 tokenAmount, uint256 ethAmount, uint256 liquidity) = IDexRouter(swapRouter)
            .addLiquidityETH{value: address(this).balance}(
            address(this), // token
            lp, // token desired
            lp, // token min
            address(this).balance, // eth min
            address(0), // lp to 0x0 addres thus totally burned
            block.timestamp + 1 days // deadline
        );

        emit LunchEvent(address(this), tokenAmount, ethAmount, liquidity);
    }


    function isContract(address account) internal view returns (bool) {
        return account.code.length > 0;
    }


    function _update(
        address from,
        address to,
        uint256 value
    ) internal override(ERC20) {
        // if not lpDeployed, only allow refund
        if (!lpDeployed) {
            if (to == address(this) && from != address(0)) {
                // refund deprecated
            } else {
                // if it is not refund operation, check and revert.
                if (from != address(0) && from != address(this)) {
                    // if it is not INIT action, revert. from address(0) means INIT action. from address(this) means mint action.
                    revert("all tokens are locked until launch.");
                }
            }
        }
        super._update(from, to, value);
        if(!lpDeployed){
            if (to == address(this) && from != address(0)) {
                _refund(from, value);
            }
        }        
    }


     function _refund(address from, uint256 value) internal nonReentrant {
        require(!lpDeployed, "Mint is over");
        require(!isContract(from), "cannot refund to contract");
        require(from == tx.origin, "cannot refund to contract.");
        require(value >= amountPerUnits, "value not match");
        require(value % amountPerUnits == 0, "value not match");

        uint256 _eth = (value / amountPerUnits) * price * (10000 - fundRatio) / 10000;
        require(_eth > 0);

        minted -= value;
        payable(from).transfer(_eth);
        emit RefundEvent(from, value, _eth);
    }

    function transfer(address to, uint256 value) public override (ERC20) returns (bool) {
        address owner = _msgSender();
        _transfer(owner, to, value);
        return true;
    }

   
    function transferFrom(address from, address to, uint256 value) public override (ERC20) returns (bool) {
        address spender = _msgSender();
        _spendAllowance(from, spender, value);
        _transfer(from, to, value);
        return true;
    }
}